            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="BlockResultPacket">
        <Properties>
            <Message Name="Hash" Id="0" Type="Hash" />
            <Bytes Name="Value" Id="1" IsRecycled="true" />
        </Properties>
    </Class>

    <Class Modifier="internal" Name="BroadcastMetadatasRequestPacket">
        <Properties>
            <Array Name="Signatures" Id="0" MaxLength="1024 * 8">
                <Element>
                    <Message Type="Signature" />
                </Element>
            </Array>
        </Properties>
