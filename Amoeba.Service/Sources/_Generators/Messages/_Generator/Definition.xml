            <Message Name="Cash" Id="4" Type="Cash" IsNullable="true" />
            <Message Name="Certificate" Id="5" Type="Certificate" IsNullable="true" />
        </Properties>
    </Class>

    <Class Modifier="internal" Name="UnicastMetadata">
        <Properties>
            <String Name="Type" Id="0" MaxLength="256" />
